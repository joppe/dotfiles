{
  "name": "dotfiles",
  "version": "1.0.0",
  "scripts": {
    "copy:alacritty": "cp ./alacritty/.alacritty.yml $HOME/.alacritty.yml",
    "copy:fish": "cp -r ./fish/* $HOME/.config/fish/",
    "copy:git": "cp ./git/.gitconfig $HOME/.gitconfig",
    "copy:i3": "cp ./i3/config $HOME/.config/config",
    "copy:i3status": "cp ./i3status-rust/config.toml $HOME/.config/i3status-rust/config.toml"
  }
}
